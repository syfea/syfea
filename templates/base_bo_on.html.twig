{% extends 'base_bo.html.twig' %}

{% block body %}
    <!-- HEADER MOBILE-->
    <header class="header-mobile d-block d-lg-none">
        <div class="header-mobile__bar">
            <div class="container-fluid">
                <div class="header-mobile-inner">
                    <a class="logo" href="{{ path('admin') }}">
                        Syfea - Amdin
                    </a>
                    <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                    </button>
                </div>
            </div>
        </div>
        <nav class="navbar-mobile">
            <div class="container-fluid">
                <ul class="navbar-mobile__list list-unstyled">
                    <li class="has-sub">
                        <a class="js-arrow" href="#">
                            <i class="fas fa-tachometer-alt"></i>Dashboard
                        </a>
                    </li>
                    <li>
                        <a href="chart.html">
                            <i class="fas fa-chart-bar"></i>Infos perso.
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('job_index') }}">
                            <i class="fas fa-table"></i>Expérience pro
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('school_index') }}">
                            <i class="far fa-check-square"></i>Educations
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('project_index') }}">
                            <i class="fas fa-calendar-alt"></i>Projets
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('service_index') }}">
                            <i class="fa fa-bolt"></i>Service
                        </a>
                    </li>
                    <li>
                        <a href="map.html">
                            <i class="fas fa-map-marker-alt"></i>Articles
                        </a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <!-- END HEADER MOBILE-->

    <!-- MENU SIDEBAR-->
    <aside class="menu-sidebar d-none d-lg-block">
        <div class="logo">
            <a href="{{ path('admin') }}">
                Syfea
            </a>
        </div>
        <div class="menu-sidebar__content js-scrollbar1">
            <nav class="navbar-sidebar">
                <ul class="list-unstyled navbar__list">
                    <li class="active has-sub">
                        <a class="js-arrow" href="#">
                            <i class="fas fa-tachometer-alt"></i>Dashboard
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('admin_perso') }}">
                            <i class="fas fa-user-plus"></i>Infos perso.</a>
                    </li>
                    <li>
                    <a href="{{ path('job_index') }}">
                            <i class="fa fa-tasks"></i>Expérience pro</a>
                    </li>
                    <li>
                        <a href="{{ path('school_index') }}">
                            <i class="fa fa-graduation-cap"></i>Educations</a>
                    </li>
                    <li>
                        <a href="{{ path('project_index') }}">
                            <i class="fa fa-file" aria-hidden="true"></i>Projets
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('service_index') }}">
                            <i class="fa fa-bolt" aria-hidden="true"></i>Services
                        </a>
                    </li>
                    <li>
                        <a href="map.html">
                            <i class="fa fa-file-text"></i>Articles</a>
                    </li>
                </ul>
            </nav>
        </div>
    </aside>
    <!-- END MENU SIDEBAR-->

    <!-- PAGE CONTAINER-->
    <div class="page-container">
        <!-- HEADER DESKTOP-->
        <header class="header-desktop">
            <div class="section__content section__content--p30">
                <div class="container-fluid">
                    <div class="header-wrap">
                        <form class="form-header" action="" method="POST">

                        </form>
                        <div class="header-button">
                            <div class="noti-wrap"></div>
                            <div class="account-wrap">
                                <div class="account-item clearfix js-item-menu">
                                    <div class="content">
                                        <a class="js-acc-btn" href="#">{{ app.user.username }}</a>
                                    </div>
                                    <div class="account-dropdown js-dropdown">
                                        <div class="info clearfix">
                                            <div class="content">
                                                <h5 class="name">
                                                    <a href="#">{{ app.user.username }}</a>
                                                </h5>
                                                <span class="email">{{ app.user.email }}</span>
                                            </div>
                                        </div>
                                        <div class="account-dropdown__footer">
                                            <a href="{{ path('fos_user_security_logout') }}"><i class="zmdi zmdi-power"></i>Logout</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <div class="main-content">
        {% block content %}{% endblock %}
        </div>
    </div>
{% endblock %}